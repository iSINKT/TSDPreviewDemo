<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="serilog:minimum-level" value="Verbose" />

    <add key="serilog:using:File" value="Serilog.Sinks.File, Version=2.0.0.0, Culture=neutral, PublicKeyToken=24c2f752a8e58a10" />
    <!--<add key="serilog:using:Trace" value="Serilog.Sinks.Trace, Version=2.0.0.0, Culture=neutral, PublicKeyToken=24c2f752a8e58a10" />-->

    <add key="serilog:write-to:File" />
    <add key="serilog:write-to:File.path" value="log{Date}.txt" />
    <add key="serilog:write-to:File.rollingInterval" value="Hour" />
    <add key="serilog:write-to:File.outputTemplate" value="[{Level:u3}] {Message:lj} {ThreadId}{NewLine}{Exception}" />

    <!--<add key="serilog:write-to:Trace" />-->
  </appSettings>
</configuration>